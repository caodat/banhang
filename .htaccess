Options +FollowSymLinks

RewriteEngine On

RewriteCond %{REQUEST_FILENAME} !-f

RewriteCond %{REQUEST_FILENAME} !-f

Rewriterule ^(.*)$ index.php?/$1 [L,QSA]
<IfModule mod_php5.c>
php_value upload_max_filesize 10000000
php_value post_max_size 11000000
php_value memory_limit 12000000
php_value max_input_time 40
</IfModule>
